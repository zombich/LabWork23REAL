<Window x:Class="LabWork23REAL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabWork23REAL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="16">
    <StackPanel>
        <DockPanel>
        <Label>Номер игры</Label>
        <TextBox x:Name="GameIdTextBox"/>
        </DockPanel>
        <Button Click="AddLogoButton_Click">Добавить иконку</Button>
        <Button Click="AddScreenshotButton_Click">Добавить скриншот</Button>
    </StackPanel>
</Window>
